{
  "name": "flipboxfactory/craft-sortable-associations",
  "version": "1.0.0",
  "description": "Simple Craft CMS source-target relationships",
  "keywords": [
    "craft cms",
    "flipbox",
    "elements"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "Flipbox Factory",
      "email": "hello@flipboxfactory.com"
    }
  ],
  "require": {
    "php": ">=7.0",
    "craftcms/cms": "~3.0.0",
    "flipboxfactory/craft-ember": "~1.0.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^5.0",
    "squizlabs/php_codesniffer": "^2.0"
  },
  "autoload": {
    "psr-4": {
      "flipbox\\craft\\sortable\\associations\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "flipbox\\craft\\sortable\\associations\\tests\\": "tests/src/"
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.0.x-dev"
    }
  }
}